{
  "$schema": "https://json.schemastore.org/tsconfig",
  "//": "使用本地schema路径替代网络schema以避免加载问题",
  "compilerOptions": {

    "target": "ESNext",
    "lib": ["DOM", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
